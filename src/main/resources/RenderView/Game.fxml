<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane prefHeight="690.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="GameManager.GameController">

    <children>
        <Pane fx:id="gamePane" prefHeight="690.0" prefWidth="600.0">
            <children>
                <Rectangle fx:id="paddleRect" arcHeight="5.0" arcWidth="5.0"
                           fill="#0b79e04f" height="22.0"
                           layoutX="231.0" layoutY="654.0"
                           stroke="BLACK" strokeType="INSIDE" width="328.0" />

                <Circle fx:id="ballCircle" centerX="300.0" centerY="400.0"
                        fill="#eb7752eb" radius="26.0" />

                <Pane fx:id="brickPane" layoutX="1.0" layoutY="2.0" prefHeight="346.0" prefWidth="600.0">
                    <children>
                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#1fff8d8a"
                                   height="29.0" stroke="BLACK" strokeType="INSIDE" width="66.0" />
                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#1fff8d8a"
                                   height="29.0" layoutX="66.0" stroke="BLACK"
                                   strokeType="INSIDE" width="66.0" />
                    </children>
                </Pane>
            </children>
        </Pane>
    </children>
</AnchorPane>
